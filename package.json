{
  "name": "uw-pico-monorepo",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run start --prefix backend\" \"npm run start --prefix frontend\"",
    "build": "npm run build --prefix frontend",
    "serve": "npm run build --prefix frontend && serve -s frontend/build",
    "backend": "npm run start --prefix backend",
    "frontend": "npm run start --prefix frontend"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "kill-port": "^2.0.1",
    "nodemon": "^3.1.10",
    "serve": "^14.2.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "morgan": "^1.10.1"
  }
}
